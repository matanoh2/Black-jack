<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StartController">
   <children>
      <Label layoutX="133.0" layoutY="33.0" text="Enter your name:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="playBtn" layoutX="154.0" layoutY="133.0" mnemonicParsing="false" onAction="#play" prefHeight="39.0" prefWidth="91.0" text="PLAY" />
      <Label fx:id="errorLabel" layoutX="126.0" layoutY="113.0" textFill="#c33131">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="playerName" layoutX="88.0" layoutY="73.0" onKeyTyped="#onKeyTyped">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
